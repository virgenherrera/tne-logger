{
  "name": "@tne/logger",
  "version": "1.3.0",
  "description": "Common logger for Node.js projects",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "preferGlobal": false,
  "engines": {
    "node": ">=6.14.4",
    "npm": ">=3.10.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/virgenherrera/tne-logger"
  },
  "keywords": [
    "typescript",
    "node",
    "winston",
    "sysLog",
    "dailyRotation"
  ],
  "author": "Hugo Virgen",
  "contributors": [
    {
      "name": "Hugo Virgen",
      "email": "virgenherrera@gmail.com",
      "url": "https://github.com/virgenherrera/"
    }
  ],
  "homepage": "https://github.com/virgenherrera/tne-logger#readme",
  "bugs": {
    "url": "https://github.com/virgenherrera/tne-logger/issues"
  },
  "license": "MIT",
  "scripts": {
    "test": "nyc mocha",
    "prepublish": "npm run build",
    "build": "npm run cleanup && npm audit && npm run tslint && npm run tsc && npm run test ",
    "cleanup": "rimraf dist .nyc_output coverage",
    "tslint": "./node_modules/.bin/tslint -c ./tslint.json -p ./tsconfig.json",
    "tsc": "./node_modules/.bin/tsc",
    "tsc:watch": "./node_modules/.bin/tsc -w --sourceMap"
  },
  "dependencies": {
    "@tne/common": "^1.0.1",
    "flatted": "^2.0.0",
    "rimraf": "^2.6.3",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.8.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.14.4",
    "@types/rimraf": "^2.0.2",
    "@types/winston": "^2.4.4",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "moment": "^2.24.0",
    "nyc": "^13.3.0",
    "source-map-support": "^0.5.12",
    "ts-node": "^7.0.0",
    "tslint": "^5.15.0",
    "typescript": "^3.4.3"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/**",
      "coverage/**",
      "dist/**"
    ],
    "reporter": [
      "html",
      "text-summary"
    ],
    "all": true
  }
}
